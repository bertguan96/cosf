# CosF 环境变量配置示例
# 复制此文件为 .env 并修改相应配置

# MySQL 配置
MYSQL_ROOT_PASSWORD=your_secure_password
MYSQL_DSN=root:your_secure_password@tcp(mysql:3306)/cosf?charset=utf8mb4&parseTime=True&loc=Local

# Redis 配置
REDIS_ADDR=redis:6379

# gRPC 日志配置
GRPC_GO_LOG_VERBOSITY_LEVEL=99
GRPC_GO_LOG_SEVERITY_LEVEL=info

# 应用配置
APP_ENV=production
LOG_LEVEL=info
